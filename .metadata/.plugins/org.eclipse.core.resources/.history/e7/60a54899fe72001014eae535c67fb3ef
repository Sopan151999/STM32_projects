#ifndef INC_GLOBEL_DATA_H_
#define INC_GLOBEL_DATA_H_


typedef struct STM_CAN_0x600 {
	uint8_t PA5_led : 1;
	uint8_t PA6_led : 1;
	uint8_t data1   : 1;
	uint8_t data2   : 1;
	uint8_t data3   : 1;
	uint8_t data4   : 1;
	uint8_t data5   : 1;
	uint8_t data6   : 1;
	uint8_t ADC_data   ;
	uint16_t PWM_data;
	uint16_t plm_data;

};
#define STM_ID_0x801 0x801U
#define STM_ID_0x102 0x102U
#define STM_ID_0x403 0x403U
#define STM_ID_0x550 0x550U
#define STM_ID_0x561 0x561U
#define STM_ID_0x588 0x588U
#define STM_ID_0x589 0x589U
#define STM_ID_0x581 0x581U
#endif /* INC_GLOBEL_DATA_H_ */
